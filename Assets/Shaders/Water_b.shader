				float simplePerlin2D8 = snoise( panner10 );
				float2 panner39 = ( mulTime12 * _WaveDirection + ( WaveTileUV36 * float4( 0.1,0.1,0,0 ) ).xy);
				float simplePerlin2D40 = snoise( panner39 );
				float temp_output_42_0 = ( simplePerlin2D8 + simplePerlin2D40 );
				float3 WaveHeight48 = ( ( float3(0,1,0) * _WaveHeight ) * temp_output_42_0 );
				
